<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <List
        headerText="{i18n>invoiceListTitle}"
        class="saUiResponsiveMargin"
        width="auto"
        items="{invoice>/}">
        <items>
            <ObjectListItem
                title="{invoice>title} x {invoice>completed}" />
        </items>
    </List>
</mvc:View>